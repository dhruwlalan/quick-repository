{
   "name": "quick-repository",
   "version": "1.1.6",
   "description": "Quickly create a repository locally & also host it on github, all via the command line!",
   "main": "lib/index.js",
   "bin": {
      "qr": "bin/quickrepository.js"
   },
   "engines": {
      "node": ">=10"
   },
   "files": [
      "bin",
      "lib"
   ],
   "scripts": {
      "start": "babel --watch src --out-dir lib --extensions \".ts\"",
      "build": "babel src --out-dir lib --extensions \".ts\""
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/dhruwlalan/quick-repository.git"
   },
   "keywords": [
      "commander",
      "inquirer",
      "cli",
      "utility",
      "octokit",
      "git",
      "github"
   ],
   "author": "dhruw lalan <dhruwlalan22@gmail.com>",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/dhruwlalan/quick-repository/issues"
   },
   "homepage": "https://github.com/dhruwlalan/quick-repository#readme",
   "dependencies": {
      "@octokit/rest": "^18.5.3",
      "colorette": "^1.2.2",
      "commander": "^7.2.0",
      "configstore": "^5.0.1",
      "execa": "^5.0.0",
      "inquirer": "^8.0.0",
      "ora": "^5.4.0",
      "update-notifier": "^5.1.0"
   },
   "devDependencies": {
      "@babel/cli": "^7.13.16",
      "@babel/core": "^7.13.16",
      "@babel/preset-env": "^7.13.15",
      "@babel/preset-typescript": "^7.13.0",
      "@types/inquirer": "^7.3.1",
      "@types/update-notifier": "^5.0.0",
      "@typescript-eslint/eslint-plugin": "^4.22.0",
      "@typescript-eslint/parser": "^4.22.0",
      "eslint": "^7.24.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^3.4.0",
      "prettier": "^2.2.1",
      "typescript": "^4.2.4"
   }
}
